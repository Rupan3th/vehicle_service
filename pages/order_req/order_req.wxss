/* pages/order_req/order_req.wxss */

page {
  background-color: #f7f7f7;
  height: 100%;
}

.page{
  /* background-color: #f7f7f7; */
  /* position: absolute;
  top:0;
  bottom:0; */
  width: 100%;
}
.weui-cells__title{
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  padding-left:30px;
  padding-right:30px;
  color:#666666;
  font-size:14px;
  /* background-color: red; */
}
/* 虚拟键盘 */
.keyboard{
  height: auto;
  background: #ffffff;
  position: fixed;
  top: 20rpx;
  width: 80%;  
  margin-left: 7%;
  z-index: 100;
  border-radius: 20rpx;
  padding: 10rpx 20rpx;
}
.keyboard-item{    
}
/* 关闭虚拟键盘 */
.keyboardClose{
  height: 80rpx;  
  display: flex;
  flex-direction: row;
}
.keyboard_title{ 
  font-size: 40rpx;
  margin-top: 4rpx;
  margin-left: 10rpx;
  color: #333333;
  font-weight: 400;
}
.keyboardClose_btn{      
  margin-left: auto;
  margin-top: 10rpx;
  margin-right: 10rpx;
  color: #999999
}
/* 虚拟键盘-省缩写 */

/* 虚拟键盘-行 */
.keyboard-line{
  margin:0 auto;
  text-align: center;
}
.iscarnumber .keyboard-line{
  text-align: left;
  margin-left: 5rpx;
}
/* 虚拟键盘-单个按钮 */
.keyboard-btn{
  font-size: 28rpx;
  color: #333333;
  background: #fff;
  display: inline-block;
  padding:26rpx 0; 
  width: 100rpx;  
  text-align: center;
  box-shadow: 0rpx 0rpx 0rpx 2rpx #D9D9D9;
  border-radius: 10rpx;
  margin:10rpx 10rpx;
}

.keyboard-num{
  height: auto;
  background: #ffffff;
  position: fixed;
  bottom:0;
  width: 100%;
  left:0;
  z-index: 100;
  border-top: 4rpx solid #D9D9D9;
  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx;
}
.keyboard-num-item{
  padding:10rpx 0 5rpx 0;
  position: relative;
  display: block;
}
.keyboard-num-btn{
  font-size: 17px;
  color: #333333;
  background: #fff;
  display: inline-block;
  padding:18rpx 0; 
  width: 63rpx;
  text-align: center;
  box-shadow: 2rpx 2rpx 2rpx 2rpx #999999;
  border-radius: 10rpx;
  margin:5rpx 6rpx;
}
/* 虚拟键盘-删除按钮 */
.keyboard-del{
  font-size: 17px;
  color: #333333;
  background: #A7B0BC;
  display: inline-block;
  padding:16rpx 55rpx;
  box-shadow: 2rpx 2rpx 2rpx 4rpx #999999;
  border-radius: 10rpx;
  margin:5rpx;
  position: absolute;
  bottom:6rpx;
  right: 16rpx;
}
.keyboard-del-font{
  font-size:34rpx;
}

/* 车牌号码 */
.carNumber-items{
  /* background-color: blue; */
  display: flex;  
  flex-direction: row;
  text-align: center;
  /* margin-left: 30px;
  margin-right: 30px; */
  color: #333333;
}
.carNumber-items-box{
  /* background-color: blue; */
  width: 90rpx;
  height: 70rpx;   
  display: flex;  
  flex-direction: row;
  margin-left: 65rpx;
  align-items: center;
}
.carNumber-items-box-list{  
  display: flex;  
  align-items: center;
  height: 70rpx;    
  font-size: 14px;  
  color:#333333;
}

.carNumber-item{
  background-color:white;
  width: 64rpx; 
  height: 64rpx;
  font-size: 14px;
  text-align: center;
  /* border: 2rpx solid #CCCCCC; */
  border-radius: 4rpx;
  line-height: 64rpx;
  display: inline-block;
  margin:0 7rpx;
  vertical-align: middle;
  box-sizing: border-box;
}
/* 新能源 */
.carNumber-item-newpower{
  border: 2rpx dashed #A8BFF3;
  background-color: #F6F9FF;
  color: #A8BFF3;
  font-size: 10px;
  line-height: 30rpx;
}
.carNumber-newpower-add{
  font-size: 14px;
}

/* 确认按钮 */
.carNumberBtn{  
  border-radius: 50rpx;
  margin:100rpx 40rpx 30rpx;
}
