/* select-brand */
.cells_type_a{
  /* background-color: blue; */
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 38rpx;
  margin-bottom: 38rpx;
  font-size: 14px;
  height: 72rpx;
}
.weui-cells__contents{
  /* background-color: green; */
  display: flex;
  flex-direction: row;
  margin-left: auto;
  margin-right: 16px;
  color: #333333;
  font-size: 14px;
  align-items: center;
  max-width: 440rpx;
  overflow: hidden;
}

.dropdown-arrow{
  border: 5px solid transparent;
  border-top: 6px solid #D9D9D9;
  width: 0;
  height: 0;
  content: ' ';
  margin-top: 6px;
}
.navigator-text {
  flex: 1;
}
.navigator-arrow {
  /* padding-right: 13px; */
  padding-right: 13px;  
  color: #D9D9D9;
}
.navigator-arrow:after {
  content: " ";
  display: inline-block;
  /* height: 9px;
  width: 9px;
  border-width: 1px 1px 0 0; */
  height: 9px;
  width: 9px;
  border-width: 1px 1px 0 0;
  border-color: var(--weui-FG-2);
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);  
  top: 50%;
  /* margin-top: -4px; */
  margin-top: -4px;
  /* right: 14px; */
  right: 14px;
}

